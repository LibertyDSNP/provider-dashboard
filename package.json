{
  "name": "provider-dashboard",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest",
    "test:e2e": "vitest run --dir ./test/e2e",
    "test:stake": "vitest stake",
    "test:ci": "vitest run --dir ./test/unit-and-integration --coverage",
    "test:ui": "vitest --ui",
    "test:watch": "vitest src",
    "coverage": "vitest run --coverage",
    "lint": "npx eslint src test",
    "prettier": "npx prettier --write --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss test src",
    "prettier:check:ci": "npx prettier --check --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss test src"
  },
  "devDependencies": {
    "@polkadot/types-codec": "^10.11.2",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/adapter-static": "^2.0.2",
    "@sveltejs/kit": "1.20.4",
    "@sveltejs/vite-plugin-svelte": "^2.4.3",
    "@tailwindcss/forms": "^0.5.7",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/svelte": "^4.0.5",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "@vitest/coverage-v8": "^1.1.3",
    "@vitest/ui": "^1.1.3",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-plugin-svelte": "^2.35.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^23.2.0",
    "postcss": "^8.4.33",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "prettier-plugin-tailwindcss": "^0.5.3",
    "sass": "^1.69.7",
    "svelte": "^4.2.8",
    "svelte-check": "^3.6.2",
    "tailwindcss": "^3.4.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^4.4.8",
    "vitest": "^1.2.2"
  },
  "type": "module",
  "dependencies": {
    "@frequency-chain/api-augment": "^1.10.0",
    "@polkadot/api": "^10.11.2",
    "@polkadot/extension-dapp": "^0.46.6",
    "@polkadot/extension-inject": "^0.46.6",
    "@polkadot/keyring": "^12.6.2",
    "@polkadot/types": "^10.11.2",
    "@polkadot/util": "^12.6.2",
    "jsdom": "^23.2.0",
    "rxjs": "^7.8.1"
  },
  "stackblitz": {
    "startCommand": "npm run test:ui"
  }
}
